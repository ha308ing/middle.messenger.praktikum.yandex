{
  "name": "messenger",
  "description": "Yandex Praktikum Project Messenger",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run prebuild && vite build --emptyOutDir",
    "preview": "vite preview --open",
    "server": "node server.js",
    "start": "npm-run-all -s build server",
    "prebuild": "tsc --noEmit",
    "eslint": "eslint .",
    "prettier": "prettier ./**/*.{ts,js,html,css,scss} --check",
    "stylelint": "stylelint ./src/**/*.{scss,css}",
    "test": "jest",
    "lint": "npm-run-all -p eslint stylelint --silent",
    "fix-code": "npm-run-all -s prettier lint --silent",
    "validate": "npm-run-all -s fix-code test",
    "pre-commit": "npm run validate",
    "pre-push": "npm run validate",
    "prepare": "husky"
  },
  "dependencies": {
    "express": "4.19.2",
    "handlebars": "4.7.8",
    "open": "10.1.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/user-event": "14.5.2",
    "@types/jsdom": "21.1.6",
    "@types/uuid": "9.0.8",
    "chai": "5.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "39.1.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "mocha": "10.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "sass": "1.72.0",
    "stylelint": "16.3.1",
    "stylelint-config-standard-scss": "13.0.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.3",
    "vite": "5.2.7",
    "vite-plugin-handlebars": "2.0.0"
  }
}
